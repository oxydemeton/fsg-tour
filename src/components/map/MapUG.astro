---
import Door from "./Door.astro";
import Zoomable from "../ZoomableContainer.astro";
const base = import.meta.env.BASE_URL
---
<Zoomable>
    <svg viewBox="0 0 4367 3087">
       
    </svg>
</Zoomable>

<style>
    svg {
        isolation: isolate;
    }
    line, polyline, path {
        stroke: currentColor;
        stroke-width: 8;
        fill: transparent;
        padding: 0;
        margin: 0;
    }
    a {
        padding: 0;
        margin: 0;
        z-index: -10;
    }
    a:hover path {
        fill: var(--highlight-bg-color);
        border: none;
        stroke-width: 18;
    }
    a:focus-visible {
        border: none;
        outline: none;
        path {
            fill: var(--highlight-bg-color);
        }
        z-index: 10;
    }
    a:hover {
        z-index: 10;
    }
    text {
        font-size: 512;
        fill: currentColor;
    }
</style>